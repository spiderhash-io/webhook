# etcd Config Backend Configuration
# Copy this file to .env and adjust values as needed

# Config backend (must be "etcd" to use etcd)
CONFIG_BACKEND=etcd

# etcd Connection
ETCD_HOST=etcd
ETCD_PORT=2379
ETCD_PREFIX=/cwm/
ETCD_NAMESPACE=default

# etcd Authentication (optional, uncomment if auth enabled)
# ETCD_USERNAME=root
# ETCD_PASSWORD=changeme

# Webhook Authorization Token (used in test webhooks)
WEBHOOK_TOKEN=test_token_123
