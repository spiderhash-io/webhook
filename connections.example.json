{
    "rabbitmq_local": {
        "type": "rabbitmq",
        "host": "localhost",
        "port": 5672,
        "user": "guest",
        "pass": "guest"
    },
    "rabbitmq_prod": {
        "type": "rabbitmq",
        "host": "{$RABBITMQ_HOST}",
        "port": "{$RABBITMQ_PORT:5672}",
        "user": "{$RABBITMQ_USER}",
        "pass": "{$RABBITMQ_PASS}"
    },
    "redis_local": {
        "type": "redis-rq",
        "host": "localhost",
        "port": 6379,
        "db": 0
    },
    "redis_prod": {
        "type": "redis-rq",
        "host": "{$REDIS_HOST}",
        "port": "{$REDIS_PORT:6379}",
        "db": "{$REDIS_DB:0}"
    },
    "kafka_local": {
        "type": "kafka",
        "bootstrap_servers": "localhost:9092"
    },
    "kafka_prod": {
        "type": "kafka",
        "bootstrap_servers": "{$KAFKA_BOOTSTRAP_SERVERS}"
    },
    "s3_storage": {
        "type": "s3",
        "aws_access_key_id": "{$AWS_ACCESS_KEY_ID}",
        "aws_secret_access_key": "{$AWS_SECRET_ACCESS_KEY}",
        "region": "us-east-1"
    },
    "clickhouse_local": {
        "type": "clickhouse",
        "host": "localhost",
        "port": 9000,
        "database": "default",
        "user": "default",
        "password": ""
    },
    "clickhouse_prod": {
        "type": "clickhouse",
        "host": "{$CLICKHOUSE_HOST}",
        "port": "{$CLICKHOUSE_PORT:9000}",
        "database": "{$CLICKHOUSE_DATABASE:default}",
        "user": "{$CLICKHOUSE_USER:default}",
        "password": "{$CLICKHOUSE_PASSWORD:}"
    },
    "clickhouse_analytics": {
        "type": "clickhouse",
        "host": "{$CLICKHOUSE_HOST}",
        "port": 9000,
        "database": "webhook_analytics",
        "user": "{$CLICKHOUSE_USER}",
        "password": "{$CLICKHOUSE_PASSWORD}"
    },
    "mqtt_local": {
        "type": "mqtt",
        "host": "localhost",
        "port": 1883,
        "username": "",
        "password": "",
        "client_id": "webhook-module",
        "keepalive": 60,
        "mqtt_version": "3.1.1"
    },
    "mqtt_prod": {
        "type": "mqtt",
        "host": "{$MQTT_HOST}",
        "port": "{$MQTT_PORT:1883}",
        "username": "{$MQTT_USERNAME}",
        "password": "{$MQTT_PASSWORD}",
        "client_id": "{$MQTT_CLIENT_ID:webhook-module}",
        "keepalive": 60,
        "mqtt_version": "3.1.1"
    },
    "mqtt_tls": {
        "type": "mqtt",
        "host": "{$MQTT_HOST}",
        "port": "{$MQTT_TLS_PORT:8883}",
        "username": "{$MQTT_USERNAME}",
        "password": "{$MQTT_PASSWORD}",
        "client_id": "webhook-module",
        "keepalive": 60,
        "mqtt_version": "3.1.1",
        "tls": true,
        "tls_ca_cert_file": "{$MQTT_CA_CERT_FILE:}",
        "tls_cert_file": "{$MQTT_CERT_FILE:}",
        "tls_key_file": "{$MQTT_KEY_FILE:}",
        "tls_insecure": false
    },
    "mqtt_shelly": {
        "type": "mqtt",
        "host": "{$MQTT_HOST}",
        "port": "{$MQTT_PORT:1883}",
        "username": "{$MQTT_USERNAME}",
        "password": "{$MQTT_PASSWORD}",
        "client_id": "webhook-shelly",
        "keepalive": 60,
        "mqtt_version": "3.1.1",
        "tls": false
    },
    "mqtt_sonoff": {
        "type": "mqtt",
        "host": "{$MQTT_HOST}",
        "port": "{$MQTT_PORT:1883}",
        "username": "{$MQTT_USERNAME}",
        "password": "{$MQTT_PASSWORD}",
        "client_id": "webhook-sonoff",
        "keepalive": 60,
        "mqtt_version": "3.1.1"
    },
    "postgres_local": {
        "type": "postgresql",
        "host": "localhost",
        "port": 5432,
        "database": "webhook_db",
        "user": "postgres",
        "password": "",
        "pool_min_size": 2,
        "pool_max_size": 10,
        "ssl": false
    },
    "postgres_prod": {
        "type": "postgresql",
        "host": "{$POSTGRES_HOST}",
        "port": "{$POSTGRES_PORT:5432}",
        "database": "{$POSTGRES_DATABASE:webhook_db}",
        "user": "{$POSTGRES_USER:postgres}",
        "password": "{$POSTGRES_PASSWORD}",
        "pool_min_size": 2,
        "pool_max_size": 10,
        "ssl": true,
        "ssl_ca_cert": "{$POSTGRES_SSL_CA_CERT:}",
        "ssl_cert": "{$POSTGRES_SSL_CERT:}",
        "ssl_key": "{$POSTGRES_SSL_KEY:}"
    },
    "postgres_connection_string": {
        "type": "postgresql",
        "connection_string": "{$POSTGRES_CONNECTION_STRING:postgresql://user:pass@host:5432/db}"
    },
    "mysql_local": {
        "type": "mysql",
        "host": "localhost",
        "port": 3306,
        "database": "webhook_db",
        "user": "root",
        "password": "",
        "pool_min_size": 2,
        "pool_max_size": 10,
        "ssl": false
    },
    "mysql_prod": {
        "type": "mysql",
        "host": "{$MYSQL_HOST}",
        "port": "{$MYSQL_PORT:3306}",
        "database": "{$MYSQL_DATABASE:webhook_db}",
        "user": "{$MYSQL_USER:root}",
        "password": "{$MYSQL_PASSWORD}",
        "pool_min_size": 2,
        "pool_max_size": 10,
        "ssl": true,
        "ssl_ca_cert": "{$MYSQL_SSL_CA_CERT:}",
        "ssl_cert": "{$MYSQL_SSL_CERT:}",
        "ssl_key": "{$MYSQL_SSL_KEY:}"
    },
    "mariadb_local": {
        "type": "mysql",
        "host": "localhost",
        "port": 3306,
        "database": "webhook_db",
        "user": "root",
        "password": "",
        "pool_min_size": 2,
        "pool_max_size": 10,
        "ssl": false
    }
}