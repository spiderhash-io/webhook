{
    "rabbitmq_local": {
        "type": "rabbitmq",
        "host": "localhost",
        "port": 5672,
        "user": "guest",
        "pass": "guest"
    },
    "rabbitmq_prod": {
        "type": "rabbitmq",
        "host": "{$RABBITMQ_HOST}",
        "port": "{$RABBITMQ_PORT:5672}",
        "user": "{$RABBITMQ_USER}",
        "pass": "{$RABBITMQ_PASS}"
    },
    "redis_local": {
        "type": "redis-rq",
        "host": "localhost",
        "port": 6379,
        "db": 0
    },
    "redis_prod": {
        "type": "redis-rq",
        "host": "{$REDIS_HOST}",
        "port": "{$REDIS_PORT:6379}",
        "db": "{$REDIS_DB:0}"
    },
    "kafka_local": {
        "type": "kafka",
        "bootstrap_servers": "localhost:9092"
    },
    "kafka_prod": {
        "type": "kafka",
        "bootstrap_servers": "{$KAFKA_BOOTSTRAP_SERVERS}"
    },
    "s3_storage": {
        "type": "s3",
        "aws_access_key_id": "{$AWS_ACCESS_KEY_ID}",
        "aws_secret_access_key": "{$AWS_SECRET_ACCESS_KEY}",
        "region": "us-east-1"
    },
    "clickhouse_local": {
        "type": "clickhouse",
        "host": "localhost",
        "port": 9000,
        "database": "default",
        "user": "default",
        "password": ""
    },
    "clickhouse_prod": {
        "type": "clickhouse",
        "host": "{$CLICKHOUSE_HOST}",
        "port": "{$CLICKHOUSE_PORT:9000}",
        "database": "{$CLICKHOUSE_DATABASE:default}",
        "user": "{$CLICKHOUSE_USER:default}",
        "password": "{$CLICKHOUSE_PASSWORD:}"
    },
    "clickhouse_analytics": {
        "type": "clickhouse",
        "host": "{$CLICKHOUSE_HOST}",
        "port": 9000,
        "database": "webhook_analytics",
        "user": "{$CLICKHOUSE_USER}",
        "password": "{$CLICKHOUSE_PASSWORD}"
    }
}